  <xsl:template match="string[@id='str_plus_trait_good_eval_generosity']/@text" ><xsl:attribute name="text">{=EApLFqpT}I like that {?GENDER}she{?}he{\?} is loyal to {?GENDER}her{?}his{\?} friends and kin.</xsl:attribute></xsl:template> 
  <xsl:template match="string[@id='str_plus_trait_good_eval_honor']/@text" ><xsl:attribute name="text">{=d69bzEWk}I like that {?GENDER}she{?}he{\?} keeps {?GENDER}her{?}his{\?} word.</xsl:attribute></xsl:template> 
  <xsl:template match="string[@id='str_plus_trait_good_eval_calculating']/@text" ><xsl:attribute name="text">{=WMHA0VDg}I like that {?GENDER}she{?}he{\?} thinks before {?GENDER}she{?}he{\?} acts.</xsl:attribute></xsl:template> 
  <xsl:template match="string[@id='str_plus_trait_bad_eval_valor']/@text" ><xsl:attribute name="text">{=D4fRtJvU}I worry {?GENDER}she{?}he{\?} risks too much in battle.</xsl:attribute></xsl:template> 

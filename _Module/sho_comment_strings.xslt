  <xsl:template match="string[@id='str_plus_trait_bad_eval_generosity']/@text" ><xsl:attribute name="text">{=6DiKo17s}I think {?GENDER}she{?}he{\?} is overly loyal to {?GENDER}her{?}his{\?} friends and kin.</xsl:attribute></xsl:template> 
  <xsl:template match="string[@id='str_plus_trait_bad_eval_honor']/@text" ><xsl:attribute name="text">{=Fb2KqYPu}I dislike {?GENDER}her{?}his{\?} rigid, self-righteous sense of honor.</xsl:attribute></xsl:template> 
  <xsl:template match="string[@id='str_plus_trait_bad_eval_calculating']/@text" ><xsl:attribute name="text">{=80t8sIDG}I don't like that {?GENDER}she{?}he{\?} always uses {?GENDER}her{?}his{\?} head but never {?GENDER}her{?}his{\?} heart. You can't trust that.</xsl:attribute></xsl:template> 
  <xsl:template match="string[@id='str_minus_trait_good_eval_valor']/@text" ><xsl:attribute name="text">{=W24au7ka}I like that {?GENDER}she{?}he{\?} shows caution.</xsl:attribute></xsl:template> 
  <xsl:template match="string[@id='str_minus_trait_good_eval_mercy']/@text" ><xsl:attribute name="text">{=G7waxWxo}I like that {?GENDER}she{?}he{\?} shows no mercy to enemies.</xsl:attribute></xsl:template> 
